{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@8.0.0",
      "destination": "lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "alpinejs@3.13.3",
      "destination": "lib/alpinejs/",
      "files": [
        "cdn.js",
        "cdn.min.js"
      ]
    }
  ]
}